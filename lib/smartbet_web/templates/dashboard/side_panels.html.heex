<div id="bet-panel" class="right-panel-wrapper is-activity">
   <div class="panel-overlay"></div>
   <div class="right-panel">
      <div class="right-panel-head">
         <h3>Add New Bet</h3>
         <a class="close-panel">
            <i data-feather="chevron-right"></i>
         </a>
      </div>
      <div class="tabs-wrapper is-triple-slider is-squared">
         <div class="right-panel-body">
            <%= render( SmartbetWeb.UserBetsView, "huro_side_panel_new.html", conn: @conn, changeset: @changeset) %>
         </div>
      </div>
   </div>
</div>
<div id="edit-bet-panel" class="right-panel-wrapper is-activity">
   <div class="panel-overlay"></div>
   <div class="right-panel">
      <div class="right-panel-head">
         # TODO extraxt to a different render component
         <h3>Edit Bet</h3>
         <a class="close-panel">
            <i data-feather="chevron-right"></i>
         </a>
      </div>
      <div class="tabs-wrapper is-triple-slider is-squared">
         <div class="right-panel-body">
            <div id="team-side-tab" class="tab-content is-active">
               <div class="columns is-multiline">
                  <div class="column is-12">
                     <div class="field">
                        <label>Bet detail</label>
                        <div class="control has-icon">
                           <input type="text" class="input" placeholder="">
                           <div class="form-icon">
                              <i data-feather="info"></i>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="column is-12">
                     <div class="field">
                        <label>Event</label>
                        <div class="control has-icon">
                           <input type="text" class="input" placeholder="">
                           <div class="form-icon">
                              <i data-feather="help-circle"></i>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="column is-6">
                     <div class="field">
                        <label>Sport</label>
                        <div class="control has-icon">
                           <input type="text" class="input" placeholder="">
                           <div class="form-icon">
                              <i data-feather="loader"></i>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="column is-6">
                     <div class="field">
                        <label>Line</label>
                        <div class="control has-icon">
                           <input type="text" class="input" placeholder="">
                           <div class="form-icon">
                              <i data-feather="minus"></i>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="column is-6">
                     <div class="field">
                        <label>Risk Units</label>
                        <div class="control has-icon">
                           <input type="text" class="input" placeholder="">
                           <div class="form-icon">
                              <i data-feather="dollar-sign"></i>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="column is-6">
                     <div class="field">
                        <label>Odds</label>
                        <div class="control has-icon">
                           <input type="text" class="input" placeholder="">
                           <div class="form-icon">
                              <i data-feather="flag"></i>
                           </div>
                        </div>
                     </div>
                  </div>
                  <div class="column is-6">
                              <div class="field">
                                 <label>Type</label>
                                 <div class="control">
                                    <div class="h-select is-full-width">
                                       <div class="select-box">
                                          <span>Platform</span>
                                       </div>
                                       <div class="select-icon">
                                          <i data-feather="chevron-down"></i>
                                       </div>
                                       <div class="select-drop has-slimscroll-sm">
                                          <div class="drop-inner">
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Caliente</span>
                                                </div>
                                             </div>
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Upick</span>
                                                </div>
                                             </div>
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Betcris</span>
                                                </div>
                                             </div>
                                          </div>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                           </div>
                  <div class="column is-6">
                              <div class="field">
                                 <label>Type</label>
                                 <div class="control">
                                    <div class="h-select is-full-width">
                                       <div class="select-box">
                                          <span>Select a Type</span>
                                       </div>
                                       <div class="select-icon">
                                          <i data-feather="chevron-down"></i>
                                       </div>
                                       <div class="select-drop has-slimscroll-sm">
                                          <div class="drop-inner">
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Money line</span>
                                                </div>
                                             </div>
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Spread</span>
                                                </div>
                                             </div>
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Over / Under</span>
                                                </div>
                                             </div>
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Parlay</span>
                                                </div>
                                             </div>
                                             <div class="option-row">
                                                <input type="radio" name="hero_select">
                                                <div class="option-meta">
                                                   <span>Other</span>
                                                </div>
                                             </div>
                                          </div>
                                       </div>
                                    </div>
                                 </div>
                              </div>
                           </div>
                  <div class="column is-6">
                     <button class="button h-button is-light is-fullwidth close-panel">
                        <span class="icon">
                           <i data-feather="edit-2"></i>
                        </span>
                        <span>Cancel</span>
                     </button>
                  </div>
                  <div class="column is-6">
                     <button class="button h-button is-success is-elevated is-fullwidth is-medium">
                        <span class="icon">
                           <i class="fas fa-check"></i>
                        </span>
                        <span>Edit bet</span>
                     </button>
                  </div>
               </div>
            </div>
